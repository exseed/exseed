{
  "name": "exseed",
  "version": "0.2.2",
  "description": "A highly extensible nodejs framework",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run jscs && npm run build && npm run mocha",
    "jscs": "jscs ./src --config=./.jscsrc --verbose",
    "build": "gulp clean && gulp build",
    "mocha": "mocha --harmony ./dist/tests/api/index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/exseed/exseed.git"
  },
  "keywords": [
    "node",
    "framework",
    "extensible"
  ],
  "author": "Weng, Chih-Ping <gocreating@gmail.com> (http://github.com/gocreating)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exseed/exseed/issues"
  },
  "homepage": "https://github.com/exseed/exseed",
  "dependencies": {
    "async": "^1.5.2",
    "express": "^4.13.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "sails-memory": "^0.10.5",
    "waterline": "^0.10.26"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.1.2",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "rimraf": "^2.5.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  }
}
