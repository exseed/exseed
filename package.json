{
  "name": "exseed",
  "version": "0.1.14",
  "description": "A highly extensible nodejs framework",
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "npm run jscs_lib && npm run jscs_example && npm run build_exseed_cli && npm run link_exseed && npm run clean_db && npm run init_example && npm run mocha_example",
    "jscs_lib": "jscs ./src --config=./.jscsrc --verbose",
    "jscs_example": "jscs ./example/src --config=./example/.jscsrc --verbose",
    "build_exseed_cli": "gulp",
    "link_exseed": "npm link . && cd example && npm link exseed",
    "clean_db": "rm -rf ./example/.db/test.default.db",
    "init_example": "cd example && sd init -t && cd ..",
    "mocha_example": "cd example && sd test && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/exseed/exseed.git"
  },
  "keywords": [
    "node",
    "framework",
    "extensible"
  ],
  "author": "gocreating",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exseed/exseed/issues"
  },
  "homepage": "https://github.com/exseed/exseed",
  "bin": {
    "sd": "dist/cli/index.js"
  },
  "dependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-nodemon": "^2.0.4",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "node-libs-browser": "^0.5.0",
    "object-assign": "^4.0.1",
    "pretty-hrtime": "^1.0.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "waterline": "^0.10.26",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  }
}
